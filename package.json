{
  "name": "gympass",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/server.cjs",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "up": "docker compose up -d",
    "stop": "docker compose stop",
    "test:create-prisma-environment": "yarn link --link-folder ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "yarn link vitest-environment-prisma",
    "test": "vitest run --dir src/services",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http/",
    "vitest:watch": "vitest run --dir src/services",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fastify/cookie": "10.0.0",
    "@fastify/jwt": "9.0.0",
    "@prisma/client": "5.19.1",
    "bcryptjs": "2.4.3",
    "colors": "1.4.0",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fastify": "5.0.0",
    "npm-run-all": "4.1.5",
    "uuid": "10.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.11.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.5.5",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "2.1.1",
    "@vitest/ui": "2.1.1",
    "eslint": "9.11.0",
    "globals": "15.9.0",
    "prisma": "5.19.1",
    "supertest": "7.0.0",
    "tsup": "8.3.0",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.6.0",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.1"
  }
}
